    "description": "Doctrine 2 Entity Auditing",
    "type": "zf-module",
    "license": "MIT",
    "keywords": [
        "audit",
        "doctrine"
    ],
    "authors": [
        {
            "name": "Tom Anderson",
            "email": "tom.h.anderson@gmail.com",
            "homepage": "http://www.tomhanderson.com"
        }
