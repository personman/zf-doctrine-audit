        "zf-commons/zfc-user-doctrine-orm": "dev-master",
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "ZF\\Doctrine\\Audit\\": "src/"
        },
        "classmap": [
